     RowBox[{
      RowBox[{"n", "[", 
       RowBox[{"0", ",", "r"}], "]"}], "=", "1.01"}], ";", "\n", "     ", 
     RowBox[{"Do", "[", "\n", "\t        ", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"n", "[", 
          RowBox[{
           RowBox[{"t", "+", "1"}], ",", "r"}], "]"}], "=", 
         RowBox[{
          RowBox[{"n", "[", 
           RowBox[{"t", ",", "r"}], "]"}], "*", 
          RowBox[{"E", "^", 
           RowBox[{"(", 
            RowBox[{"r", "*", 
             RowBox[{"(", 
