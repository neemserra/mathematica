 CellChangeTimes->{{3.397952572386466*^9, 3.397952573052203*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"tmax", "=", "200"}], ";"}], "\n", "\n", 
  RowBox[{"(*", " ", 
   RowBox[{
   "solve", " ", "the", " ", "difference", " ", "equation", " ", "for", " ", 
    "values", " ", "of", " ", "r", " ", "between", " ", "0", " ", "and", " ", 
    "4", " ", "stepping", " ", "by", " ", "0.01"}], " ", "*)"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"Do", "[", "\n", "\t   ", 
