Cell["\<\
3) Discrete-time logistic exploration\
\>", "Section",
 CellChangeTimes->{
  3.3979526211858892`*^9, {3.397989611608563*^9, 3.3979896120002832`*^9}, {
   3.5564058078927097`*^9, 3.556405810587365*^9}}],

Cell["\<\
Here we will further explore the discrete logistic equation discussed in \
class and in May (1974): n[t+1]=n[t]exp[r*(1-n[t]/k)].\
\>", "Text"],

Cell[CellGroupData[{

Cell["\<\
Simulate the difference equation for r=1.0,1.4,1.8,2.2,2.6,3.0.\
\>", "Subsubsection"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Clear", "[", 
    RowBox[{"n", ",", "f"}], "]"}], ";"}], "\n", "\n", 
  RowBox[{"(*", " ", 
   RowBox[{"set", " ", "parameters"}], " ", "*)"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"r", "=", "2.8"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"n", "[", "0", "]"}], "=", "0.01"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"tmax", "=", "100"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"f", "[", "n_", "]"}], ":=", 
    RowBox[{"n", "*", 
     RowBox[{"E", "^", 
      RowBox[{"(", 
       RowBox[{"r", "*", 
        RowBox[{"(", 
         RowBox[{"1", "-", "n"}], ")"}]}], ")"}]}]}]}], ";"}], "\n", "\n", 
  RowBox[{"(*", " ", 
   RowBox[{"iterate", " ", "difference", " ", "equation"}], " ", "*)"}], 
  "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Do", "[", "\n", "\t", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"n", "[", 
        RowBox[{"t", "+", "1"}], "]"}], "=", 
       RowBox[{"f", "[", 
        RowBox[{"n", "[", "t", "]"}], "]"}]}], ";"}], "\n", "\t", ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", "tmax"}], "}"}]}], "]"}], ";"}], "\n", "\n", 
  RowBox[{"(*", " ", 
   RowBox[{"plot", " ", 
    RowBox[{"n", "[", "t", "]"}], " ", "as", " ", "a", " ", "function", " ", 
    "of", " ", "t"}], " ", "*)"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"ListPlot", "[", 
   RowBox[{
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"n", "[", "t", "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"t", ",", "0", ",", "tmax"}], "}"}]}], "]"}], ",", 
    RowBox[{"Joined", "->", "True"}], ",", 
    RowBox[{"PlotRange", "\[Rule]", "All"}], ",", 
    RowBox[{"PlotStyle", "\[Rule]", 
     RowBox[{"RGBColor", "[", 
      RowBox[{"0", ",", "1", ",", "0"}], "]"}]}], ",", 
    RowBox[{"AxesLabel", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{"\"\<t\>\"", ",", "\"\<n[t]\>\""}], "}"}]}]}], "]"}], "\n", 
  "\n", 
  RowBox[{"(*", " ", 
   RowBox[{"illustrate", " ", 
    RowBox[{"\"\<cobwebbing\>\"", " ", "--"}], " ", 
    RowBox[{"don", "'"}], "t", " ", "worry", " ", "about", " ", "the", " ", 
    "details", " ", "of", " ", "this", " ", "code"}], " ", "*)"}], 
  "\n"}], "\n", 
 RowBox[{"Show", "[", 
  RowBox[{
   RowBox[{"Plot", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"n", ",", 
       RowBox[{"f", "[", "n", "]"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"n", ",", "0", ",", 
       RowBox[{"1.05", "*", 
        RowBox[{
         RowBox[{"E", "^", 
          RowBox[{"(", 
           RowBox[{"r", "-", "1"}], ")"}]}], "/", "r"}]}]}], "}"}], ",", 
     RowBox[{"DisplayFunction", "->", "Identity"}]}], "]"}], ",", "\n", "  ", 
   
   RowBox[{"Graphics", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"RGBColor", "[", 
       RowBox[{"0", ",", "1", ",", "0"}], "]"}], ",", 
      RowBox[{"Line", "[", 
       RowBox[{"Flatten", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"n", "[", "t", "]"}], ",", 
               RowBox[{"n", "[", "t", "]"}]}], "}"}], ",", 
